{
  "manifest_version": 3,
  "name": "Cute Save Button",
  "version": "0.8.5",
  "description": "__MSG_extensionDescription__",
  "author": "Deil",
  "default_locale": "en",
  "icons": {
    "48": "bestgirl.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "content_scripts": [{
    "js": [
      "js/content-script.js"
    ],
    "matches": [
      "*://*/*"
    ],
    "run_at": "document_start",
    "all_frames": true
  }],
  "web_accessible_resources": [{
    "resources": [
      "bestgirl.png"
    ],
    "matches": [
      "<all_urls>"
    ]
  }],
  "options_ui": {
    "page": "settings.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": {
      "48": "bestgirl.png"
    }
  }
}
