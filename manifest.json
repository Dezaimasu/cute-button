{
	"manifest_version": 2,
	"name": "Cute Save Button",
	"version": "0.4.5",
	"description": "Little button to save images and webms in one click or with any hotkey. Praise cute!",
	"author": "Deil",
	"icons": {
		"48": "bestgirl.png"
	},
	"permissions": [
		"webNavigation",
		"downloads",
		"storage",
		"*://*/*"
	],
	"background": {
		"scripts": [
			"js/settings-default.js",
			"js/background.js",
			"js/download.js"
		]
	},
	"web_accessible_resources": [
		"bestgirl.png"
	],
	"options_ui": {
		"page": "settings.html",
		"browser_style": true
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"48": "bestgirl.png"
		}
	},
	"applications": {
		"gecko": {
			"id": "ochecuteextension@plaza.ink",
			"strict_min_version": "52.0"
		}
	}
}
